{
  "name": "causal/fal-protect",
  "type": "typo3-cms-extension",
  "description": "Protect everything within /fileadmin/ based on associated folder and file restrictions (visibility, user groups and dates of publication).",
  "keywords": [
    "TYPO3 CMS",
    "FAL",
    "files",
    "directories",
    "secure",
    "security",
    "protection"
  ],
  "support": {
    "docs": "https://docs.typo3.org/p/causal/fal-protect/main/en-us/",
    "issues": "https://github.com/xperseguers/t3ext-fal-protect/issues",
    "source": "https://github.com/xperseguers/t3ext-fal-protect"
  },
  "authors": [
    {
      "name": "Xavier Perseguers",
      "email": "xavier@causal.ch",
      "homepage": "https://www.causal.ch",
      "role": "Developer"
    }
  ],
  "license": "GPL-2.0-or-later",
  "require": {
    "php": ">=7.2.0 <=7.4.99 || >=8.1.0 <=8.3.99",
    "typo3/cms-core": "^10 || ^11 || ^12 || ^13",
    "lochmueller/http-range": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Causal\\FalProtect\\": "Classes/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.6.x-dev"
    },
    "typo3/cms": {
      "extension-key": "fal_protect"
    }
  },
  "config": {
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true
    }
  }
}
